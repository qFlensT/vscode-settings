{
  "apc.activityBar": {
    "position": "bottom",
    "size": 25
  },
  "apc.font.family": "Roboto Mono",
  "apc.menubar.compact": true,
  "apc.statusBar": {
    "fontSize": 10,
    "height": 25,
    "position": "editor-bottom"
  },
  "apc.stylesheet": {
    ".inline-titlebar-placeholder": "display: none !important",
    ".title-actions": "display: none !important"
  },
  "breadcrumbs.enabled": false,
  "editor.accessibilitySupport": "off",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Roboto Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.guides.indentation": false,
  "editor.lightbulb.enabled": false,
  "editor.lineHeight": 1.4,
  "editor.matchBrackets": "never",
  "editor.minimap.enabled": false,
  "editor.overviewRulerBorder": false,
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "none",
  "editor.scrollbar.horizontalScrollbarSize": 10,
  "editor.scrollbar.verticalScrollbarSize": 10,
  "editor.showFoldingControls": "never",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.includeStrings": false,
  "errorLens.fontFamily": "Roboto Mono",
  "errorLens.fontSize": "12px",
  "errorLens.fontStyleItalic": true,
  "errorLens.messageBackgroundMode": "message",
  "errorLens.statusBarIconsEnabled": true,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "off",
  "files.insertFinalNewline": true,
  "js/ts.implicitProjectConfig.checkJs": true,
  "material-icon-theme.hidesExplorerArrows": true,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.fontFamily": "RobotoMono Nerd Font Mono",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.tabs.enabled": true,
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 2,
  "workbench.colorCustomizations": {
    "sideBar.background": "#212733",
    "terminalCursor.foreground": "#ffcc66"
  },
  "workbench.colorTheme": "Field Lights",
  "workbench.editor.showTabs": "single",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tree.renderIndentGuides": "none"
}
